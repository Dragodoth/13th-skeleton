{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "bundler",
    "lib": [
      "es2022"
    ],
    "include": [
      "src/**/*.svelte",
      "src/**/*.md",
      "src/**/*.ts",
      "src/**/*.d.ts"
    ]
  }
}
